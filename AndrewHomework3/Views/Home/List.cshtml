@{
    ViewBag.Title = "Movie List";
}

@model IEnumerable<AndrewHomework3.Models.MovieModel>

<br />
<div>
    <h4>Here are the individuals currently on the waitlist</h4><br />
    <table>
        <thead>
            <tr><th>Category</th><th>Title</th><th>Year</th><th>Rating</th><th>Edited</th><th>Lent To</th><th>Notes</th></tr>
        </thead>

        <tbody>
            @foreach (AndrewHomework3.Models.MovieModel x in Model)
            {
                <tr>
                    <td>@x.Category</td>
                    <td>@x.Title</td>
                    <td>@x.Year</td>
                    <td>@x.Rating</td>
                    <td>@x.Edited</td>
                    <td>@x.LentTo</td>
                    <td>@x.Notes</td>
                </tr>
            }
        </tbody>
    </table>
</div>
